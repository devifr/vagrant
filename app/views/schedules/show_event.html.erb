<div class="innerLR add-margin-top">
	<div class="form-horizontal margin-none">

		<div class="widget widget-heading-simple widget-body-gray">
			<div class="row-fluid">				
				<div class="span12">
					<h3><i></i> Show Schedule Calendar
		  	  	<div class="buttons pull-right">
					    <%= link_to "<i></i>Back".html_safe ,schedules_path,class: "btn btn-icon btn-default" %>
					  </div>
					</h3>
				</div>
				<table class="table table-bordered none_border">
		      <thead>
		      </thead>
		      <tbody>
		        <tr>
		          <td class="none_padding">
		            <div class="row-fluid">  
		              <div class="span12">
		                <div id="calendar" data-url="<%= "/schedules/event" %>"></div>
		              </div>
			          </div>
			        </td>
			      </tr>
			    </tbody>
			  </table>
			</div>
		</div>
	</div>
</div>
<div id="calendar-modal" class="modal fade" tabindex="-1" style="display: none;">
  <div class="modal-header blue">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
      &times;
    </button>
    <h4 class="modal-title"></h4>
  </div>
  <div class="modal-body">
    <table class="table table-stripped gray-popup">
      <thead>
        <tr>
          <th class='time-popup'>TIME</th>
          <th class='task-popup'></th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
  </div>
</div>
